{
  "Creates a react component": {
    "prefix": "rec",
    "description": "Creates and export a react component",
    "body": [
      "/**",
      " * $1",
      " */",
      "export default function ${TM_FILENAME_BASE}() {",
      "\t$0",
      "}"
    ]
  },
  "Creates a new styled react component": {
    "prefix": "res",
    "description": "Creates a styled component",
    "body": [
      "/**",
      " * $1",
      " */",
      "export const ${2:Name} = styled.${3:div}`",
      "\t$0",
      "`"
    ]
  },
  "Creates a react state": {
    "prefix": "rst",
    "description": "Creates a react state",
    "body": [
      "const [${1:state}, set${1/(.*)/${1:/capitalize}/}] = useState($2)"
    ]
  },
  "Creates a nanostore": {
    "prefix": "nanomap",
    "description": "Creates a nanostore",
    "body": [
      "/**",
      " * $1",
      " */",
      "export const ${2:Name}Store = map({",
      "\t$0",
      "})"
    ]
  },
  "Creates a nanostore action of 0 arguments": {
    "prefix": "nanoact",
    "description": "Creates a nanostore action of 0 argument",
    "body": [
      "/**",
      " * $1",
      " */",
      "export const ${2:actionName} = action(${3:Name}Store, '$2', store => {",
      "\t$0",
      "})"
    ]
  },
  "Creates a nanostore action of 1 argument": {
    "prefix": "nanoact1",
    "description": "Creates a nanostore action of 1 argument",
    "body": [
      "/**",
      " * $1",
      " */",
      "export const ${2:actionName} = action(${3:Name}Store, '$2', (store, ${4:param1}: ${5:string}) => {",
      "\t$0",
      "})"
    ]
  },
  "Génére une description de module": {
    "prefix": "modoc",
    "description": "Generate a module description",
    "body": [
      "/**",
      " * @module ${TM_FILENAME_BASE}",
      " *",
      " * @description",
      " *  $1",
      " */",
      "$0"
    ]
  },
  "Creates a brand new type with documentation": {
    "prefix": "dtype",
    "body": ["/**", " * $1", " */", "${2:export} type ${3:Name} = $0"],
    "description": "Creates a documented type"
  },
  "Creates a documented function": {
    "prefix": "dfun",
    "body": [
      "/**",
      " * $1",
      " */",
      "${2:export} function ${3:name}($4): ${5:void} {",
      "\t$0",
      "}"
    ],
    "description": "A documented function"
  },
  "Creates a async documented function": {
    "prefix": "adfun",
    "body": [
      "/**",
      " * $1",
      " */",
      "${2:export} async function ${3:name}($4): Promise<${5:void}> {",
      "\t$0",
      "}"
    ],
    "description": "A documented function"
  },
  "Creates a documented arrow function": {
    "prefix": "dff",
    "body": ["/**", " * $1", " */", "${2:export} const ${3:name} = ($4) => $0"],
    "description": "A documented function"
  },
  "Creates a async documented arrow function": {
    "prefix": "adff",
    "body": [
      "/**",
      " * $1",
      " */",
      "${2:export} const ${3:name} = async ($4): Promise<${5:void}> => $0"
    ],
    "description": "A documented function"
  },
  "Creates a documented signature": {
    "prefix": "dsign",
    "body": ["/**", " * $1", " */", "${3:name}($4): ${5:void}"],
    "description": "A documented function"
  }
}
